{
  "entryPoints": [
    "src/app.ts",
    "src/auth/**/*.ts",
    "src/emails/**/*.ts",
    "src/models/**/*.ts",
    "src/shared/**/*.ts",
  ],
  "entryPointStrategy": "expand",
  "out": "docs",
  "tsconfig": "tsconfig.json",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**",
    "**/seeders/**"
  ],
  "includeVersion": true,
  "theme": "default"
}